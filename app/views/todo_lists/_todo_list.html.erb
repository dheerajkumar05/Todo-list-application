<div id="<%= dom_id todo_list %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= todo_list.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= todo_list.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Priority:</strong>
    <%= todo_list.priority %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Due date:</strong>
    <%= todo_list.due_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= todo_list.status %>
  </div>
</div>
